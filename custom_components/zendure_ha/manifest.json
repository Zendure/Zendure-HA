{
  "domain": "zendure_ha",
  "name": "Zendure Home Assistant Integration",
  "after_dependencies": [
    "zeroconf"
  ],
  "bluetooth": [
    {
      "local_name": "Zen*"
    }
  ],
  "codeowners": [
    "@fireson"
  ],
  "config_flow": true,
  "dependencies": [
    "bluetooth",
    "bluetooth_adapters",
    "mqtt"
  ],
  "documentation": "https://github.com/zendure/zendure-ha",
  "integration_type": "device",
  "iot_class": "local_polling",
  "issue_tracker": "https://github.com/zendure/zendure-ha/issues",
  "requirements": [
    "paho-mqtt==2.1.0",
    "stringcase==1.2.0"
  ],
  "single_config_entry": false,
  "version": "1.1.4",
  "zeroconf": [
    {
      "name": "zendure*",
      "type": "_http._tcp.local."
    },
    {
      "type": "_zendure._tcp.local."
    }
  ]
}